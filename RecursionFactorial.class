import java.io.*;
import java.util.*;

public class Main {
    
    
    public static void main(String[] args) throws Exception {
        Scanner src = new Scanner(System.in);
        int n = src.nextInt();
        
        int ans = factorial(n);
        System.out.println(ans);
        
        
    }
    
    public static int factorial(int n){
        if(n == 0){
            return 1;
            
        }
        
       int sum = 1;
       int value =factorial(n-1);
       sum =n*value;
      return sum;
     
       
    }
    
            
    

}