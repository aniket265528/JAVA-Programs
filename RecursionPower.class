import java.io.*;
import java.util.*;

public class Main {

    public static void main(String[] args) throws Exception {
        Scanner src =new Scanner(System.in);
        int x =src.nextInt();
        int n =src.nextInt();
        int ans = power(x,n);
        System.out.println(ans);
        
    }

    public static int power(int x, int n){
        if(n == 0){
        return 1;
        }
        
        int value =power(x,n-1);
        int pow =x*value;
        return pow;
    }

}